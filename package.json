  },
  "homepage": "https://github.com/uroborosjs/cli#readme",
  "devDependencies": {
    "@types/fs-extra": "^5.0.5",
    "@types/node": "^10.12.18",
    "@types/prompts": "^1.2.0",
    "ava": "^1.0.1",
    "ts-node": "^8.0.2",
    "ts-node-dev": "^1.0.0-pre.32",
    "tsconfig-paths": "^3.8.0",
    "tslint-immutable": "^5.0.1",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "download-git-repo": "^1.1.0",
    "fs-extra": "^7.0.1",
    "prompts": "^2.0.1",
